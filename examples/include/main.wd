include "strings"
include "io"

# Fake count
count: func(str, char) =>
    <- "Soy inteligente y no confundo scopes B)"
endfn

main: func() =>
    !(io::printLine !(strings::count "Tres tristes tigres tragaban trigo en un trigal" "i"))
    
    something => !{strings::Something}
    !(io::printLine !(something'count "Pablito clavó un clavito, ¿cuántos clavitos clavó Pablito?" "c"))

    !(io::printLine !(count "Como poco coco como, poco coco compro" "o"))
endfn

!main
