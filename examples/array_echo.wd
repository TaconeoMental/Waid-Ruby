index_element: func(num, elem) =>
    <- !(toStr num) . " -> " . elem
endfn

main: func() =>
    array => []
    while true =>
        array => array . !input
        !(printLine "Elementos")
        
        count => 0
        while count < !(length array) =>
            !(printLine !(index_element count (count @ array))) # Paréntesis están demás, pero es para claridad
            count => count + 1
        endwl
    endwl
endfn

!main
