add: func(x, y, z) =>
    <- x + y + z
endfn

generate_add: func(num) =>
    new: func(x) =>
        <- x + num
    endfn
    <- new
endfn

main: func() =>
    add_5 => !(generate_add 5)
    add_8 => !(generate_add 8)

    while true =>
        in => !input
        if in % 2 == 0 =>
            !(print !(add_5 in))
        else =>
            !(print !(add_8 in))
        endif
    endwl
endfn

!main
