fizzbuzz: func(max) =>
    count => 0
    msg => ""
    while count <= max =>
        msg => count
        if count % 3 == 0 =>
            msg => "Fizz"
        endif
        if count % 5 == 0 =>
            msg => "Buzz"
        endif
        if count % 3 == 0 and count % 5 == 0 =>
            msg => "FizzBuzz"
        endif
        !(printLine msg)
        count => count + 1
    endwl
endfn

!(print "Enter a max number: ")
!(fizzbuzz !(toNum !input))

