add: func(x, y) =>
    <- x + y
endfn

# Pseudo range function
range: func(start, end, step) =>
    arr => [..end] # Array of length "end"
    i => start
    while i < end =>
        arr[i] => i * step
        i +> 1
    endwl
    <- arr
endfn

main: func() =>
    for elem in !(range 0 !(add !input 1) 2) =>
        !print !(add elem !input)
    endfr
endfn

