add: func(x, y) =>
    <- x + y
endfn

# Pseudo range function
range: func(start, end, step) =>
    arr => []
    i => start
    while i < end:
        (i @ arr) => i * step
        i => i + 1
    endwl
    <- arr
endfn

main: func() =>
    count => 0
    range => !(range 0 !(add !(toNum !input) 1) 2)
    !(printLine range)
    while count < !(length range):
        !(printLine !(add count @ range !(toNum !input)))
        count => count + 1
    endwl
endfn

!main
